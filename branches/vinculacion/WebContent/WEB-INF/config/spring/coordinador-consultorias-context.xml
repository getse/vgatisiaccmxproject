<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="CoordinadorConsultoriasAction"
		class="mx.com.vgati.ccmx.vinculacion.coordinacion.consultorias.action.CoordinadorConsultoriasAction"
		scope="prototype">
		<property name="coordinadorConsultoriasService" ref="coordinadorConsultoriasService" />
		<property name="pyMEsService" ref="pyMEsService" />
		<property name="tractorasService" ref="tractorasService" />
		<property name="initService" ref="initService" />
		<property name="ccmxService" ref="ccmxService" />
		<property name="reportService" ref="reportService" />
	</bean>
	
	<bean id="coordinadorConsultoriasService"
		class="mx.com.vgati.ccmx.vinculacion.coordinacion.consultorias.service.imp.CoordinadorConsultoriasServiceImp">
		<property name="coordinadorConsultoriasDao" ref="coordinadorConsultoriasDao" />
	</bean>
	
	<bean id="coordinadorConsultoriasDao"
		class="mx.com.vgati.ccmx.vinculacion.coordinacion.consultorias.dao.imp.CoordinadorConsultoriasDaoJdbcImp">
		<property name="dataSource" ref="dataSource" />
	</bean>
	
</beans>