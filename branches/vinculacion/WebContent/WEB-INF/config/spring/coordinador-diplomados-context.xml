<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="CoordinadorDiplomadosAction"
		class="mx.com.vgati.ccmx.vinculacion.coordinacion.diplomados.action.CoordinadorDiplomadosAction"
		scope="prototype">
		<property name="coordinadorDiplomadosService" ref="coordinadorDiplomadosService" />
	</bean>
	
	<bean id="coordinadorDiplomadosService"
		class="mx.com.vgati.ccmx.vinculacion.coordinacion.diplomados.service.imp.CoordinadorDiplomadosServiceImp">
		<property name="coordinadorDiplomadosDao" ref="coordinadorDiplomadosDao" />
	</bean>
	
	<bean id="coordinadorDiplomadosDao"
		class="mx.com.vgati.ccmx.vinculacion.coordinacion.diplomados.dao.imp.CoordinadorDiplomadosDaoJdbcImp">
		<property name="dataSource" ref="dataSource" />
	</bean>
	
</beans>